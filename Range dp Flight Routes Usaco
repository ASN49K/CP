#include<bits/stdc++.h>
using namespace std;
using i64 = long long;
const int N=750;
bool all[N][N];
bool one[N][N];
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);
    int n;
    cin>>n;
    for(int i=0;i<n;i++)
    {
        cin.get();
        for(int j=i+1;j<n;j++)
        {
            all[i][j]=cin.get()-'0';
        }
    }
    int rez=0;
    for(int i=n-1;i>=0;i--)
    {
        for(int j=i;j<n;j++)
        {
            one[i][j]=all[i][j];
            for(int k=i+1;k<j;k++)
            {
                one[i][j] ^= (one[i][k] & all[k][j]);
            }
            rez+=one[i][j];
        }
    }
    cout<<rez;


}
