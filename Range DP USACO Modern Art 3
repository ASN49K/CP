#include <bits/stdc++.h>
#define all(x)      x.begin(),x.end()
#define bug(x)      cerr<<" ( "<<#x<<' '<<x<<"  )\n"
#define pb          push_back
using namespace std;
const int inf=1e9;
using i64=long long;
int main()
{
    int n;
    cin>>n;
    vector<int>a(n);
    for(auto &c:a)
    {
        cin>>c;
    }
    vector<vector<int>>dp(n+1,vector<int>(n+1,0));
    for(int i=0;i<n;i++)
    {
        dp[i][i]=1;
    }
    for(int l=n-1;l>=0;l--)
    {
        for(int r=l+1;r<n;r++)
        {
            dp[l][r]=min(dp[l+1][r]+(a[l]!=a[r] && a[l]!=a[l+1]) , dp[l][r-1]+(a[l]!=a[r] && a[r]!=a[r-1]));
            for(int k=l;k<r;k++)
            {
                dp[l][r]=min(dp[l][r],dp[l][k]+dp[k+1][r]);
            }
        }
    }
    cout<<dp[0][n-1];
}
